{
  "name": "nice-fns",
  "type": "module",
  "version": "0.0.0",
  "description": "A modern and nice library of JS functions.",
  "author": "RayHao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/l246804/nice-fns",
  "repository": {
    "type": "git",
    "url": "https://github.com/l246804/nice-fns"
  },
  "bugs": {
    "url": "https://github.com/l246804/nice-fns/issues"
  },
  "keywords": [
    "web",
    "node",
    "lodash",
    "function",
    "tool",
    "utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./single/*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.mjs",
      "require": "./dist/*/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "run-s gen:index build:only",
    "build:only": "unbuild",
    "gen:index": "node scripts/genIndex.mjs",
    "release": "release-it",
    "cz": "cz",
    "plop": "plop",
    "test": "vitest",
    "test:cover": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0",
    "@release-it/conventional-changelog": "^7",
    "@rhao/plop-generators": "latest",
    "@types/fs-extra": "^11.0.4",
    "commitizen": "^4",
    "eslint": "^8",
    "eslint-define-config": "^1",
    "fs-extra": "^11.1.1",
    "git-cz": "^4",
    "npm-run-all": "^4.1.5",
    "plop": "latest",
    "prettier": "^3",
    "release-it": "^16.2.1",
    "typescript": "^5.3.2",
    "unbuild": "^2",
    "vitest": "^0.34.6"
  }
}
